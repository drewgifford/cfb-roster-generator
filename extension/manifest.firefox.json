{
	"manifest_version": 3,
	"name": "CFB26 Roster Importer",
	"version": "0.2",
	"description": "Imports rosters from CFB Roster Builder",
	"permissions": ["activeTab", "tabs"],
	"background": {
		"scripts": ["background.js"]
	},
	"icons": {
		"16": "icons/icon-16.png",
		"32": "icons/icon-32.png",
		"64": "icons/icon-64.png",
		"128": "icons/icon-128.png"
	},
	"content_scripts": [
		{
			"matches": ["https://www.ea.com/games/ea-sports-college-football/team-builder/team-create/*"],
			"js": ["jquery.min.js", "inject.js"]
		}
	],
	"action": {
		"default_icon": {
			"16": "icons/icon-16.png",
			"32": "icons/icon-32.png",
			"64": "icons/icon-64.png",
			"128": "icons/icon-128.png"
		},
		"default_title": "CFB26 Roster Importer"
	},
	"web_accessible_resources": [
		{
			"resources": ["icons/*"],
			"matches": ["https://www.ea.com/*"]
		}
	]
}
